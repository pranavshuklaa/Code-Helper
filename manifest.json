{
  "manifest_version": 2,
  "name": "Best study technique",
  "version": "0.0.1",
  "icons": {
    "128": "beyonceAlways.png"
  },
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"]
  },  
  "content_scripts": [
    {
      "matches": ["<all_urls>"], 
      "js": ["content.js"]
    }
  ]
}
